{
  "name": "node.js-api-project",
  "scripts": {
    "format": "prettier --write .",
    "test": "jest",
    "lint": "eslint src/ --cache --fix",
    "build": "tsc",
    "build:dev": "tsc --watch",
    "start": "node --enable-source-maps dist/server.js",
    "start:dev": "nodemon --watch dist/",
    "dev": "npm run build && concurrently 'npm:build:dev' 'npm:start:dev'"
  },
  "engines": {
    "node": ">=16"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/camilla-devasini/Node.js-API-project.git"
  },
  "bugs": {
    "url": "https://github.com/camilla-devasini/Node.js-API-project/issues"
  },
  "homepage": "https://github.com/camilla-devasini/Node.js-API-project#readme",
  "devDependencies": {
    "@tsconfig/node18": "^1.0.1",
    "@types/express": "^4.17.14",
    "@types/jest": "^29.2.3",
    "@types/node": "^18.11.9",
    "concurrently": "^7.6.0",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "jest": "^29.3.1",
    "nodemon": "^2.0.20",
    "prettier": "2.8.0",
    "ts-jest": "^29.0.3",
    "typescript": "^4.9.3"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^5.44.0",
    "@typescript-eslint/parser": "^5.44.0",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "save-dev": "^0.0.1-security"
  }
}
